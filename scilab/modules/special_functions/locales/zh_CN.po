msgid "%s: Argument #%d: Decimal number expected.\n"
msgstr "%s：第#%d参数错误：应该是一个小数。\n"

#, c-format
msgid "%s: Argument #%d: Must be in the interval [%d, %d].\n"
msgstr "%s：第#%d个参数错误：必须处于[%d, %d]之间。\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Real expected.\n"
msgstr "%s：第%d个输入参数类型错误：应该为实数。\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: Real or complex, sparse or full "
