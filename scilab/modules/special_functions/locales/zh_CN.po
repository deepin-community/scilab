#, c-format
msgid "%s: Wrong size for input argument #%d.\n"
msgstr "%s：第%d个输入参数的维度错误。\n"

#, c-format
msgid "%s: arguments #%d and #%d have incompatible dimensions.\n"
msgstr "%s：第%d个参数和第%d个参数的维数不同。\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Real matrix expected.\n"
msgstr "%s：输入参数#%d的类型错误：应该为实数矩阵。\n"

#, c-format
msgid "%s: Wrong value for input arguments: Must be > %d.\n"
msgstr "%s：输入参数的值错误：必须 > %d。\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: Integer or vector of integers "
"expected.\n"
msgstr "%s：第%d个输入参数的类型错误：应该为整数或整数向量。\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: Integer >= %d expected.\n"
msgstr "%s：第%d个输入参数的类型错误：应该为>=%d的整数。\n"

#, c-format
msgid "%s: Only one of arg1 and arg2 may be a vector.\n"
msgstr "%s：arg1和arg2只可有一个为向量。\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Matrix with elements in [%d,%d] "
"expected.\n"
msgstr "%s：第%d个输入参数的值错误：应该为[%d,%d]区间元素值的矩阵。\n"

#, c-format
msgid "%s: overflow or underflow of an extended range number\n"
msgstr "%s：一个扩展范围数字上溢或下溢。\n"

#, c-format
msgid "%s: error number %d\n"
msgstr "%s：错误代码 %d\n"

#, c-format
msgid "%s: Wrong number of input argument: %d expected.\n"
msgstr "%s：输入参数的数目错误：应该为%d个。\n"

#, c-format
msgid "%s: Wrong type for argument #%d: A matrix expected.\n"
msgstr "%s：第%d个参数的类型错误：应该为矩阵。\n"

#, c-format
msgid "%s: Can not read input argument #%d.\n"
msgstr "%s：不能读取第%d个输入参数。\n"

#, c-format
msgid "%s: Wrong type for argument #%d: Real or complex matrix expected.\n"
msgstr "%s：第%d个参数的类型错误：应该为实数矩阵或复数矩阵。\n"

#, c-format
msgid "%s: Wrong number of input arguments: At least %d expected.\n"
msgstr "%s：输入参数数目错误：应该至少为 %d 个。\n"

#, c-format
msgid ""
"%s: Function not defined for the given argument type.\n"
"  Check arguments or define function %s for overloading.\n"
msgstr ""
"%s：给定参数类型的函数未定义。\n"
"  请检查参数或者将%s函数进行重载。\n"

