#, c-format
msgid "%s: Wrong number of input arguments: %d expected.\n"
msgstr "%s: Falsche Anzahl der Eingabeargumente: %d erwartet.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A matrix of strings expected.\n"
msgstr ""
"%s: Falscher Typ für das Eingabeargument #%d: Eine Matrix mit Zeichenketten "
"erwartet.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A scalar string expected.\n"
msgstr ""
"%s: Falscher Typ für das Eingabeargument #%d: Eine skalare Zeichenkette wird "
"erwartet.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d: A two-columns string matrix "
"expected.\n"
msgstr ""
"%s: Falscher Typ für das Eingabeargument #%d: Eine zweispaltige "
"Zeichenkettenmatrix erwartet.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d or %d expected.\n"
msgstr "%s: Falsche Anzahl der Eingabeargumente: %d oder %d erwartet.\n"

#, c-format
msgid "%s: No active coverage: can't write.\n"
msgstr "%s: Keine aktive Abdeckung: kann nicht schreiben.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: html output only.\n"
msgstr "%s: Falscher Wert für das Eingabeargument #%d: Nur html-Ausgabe.\n"

#, c-format
msgid "%s: profile is disabled.\n"
msgstr "%s: Profil ist deaktiviert.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A macro or library expected.\n"
msgstr ""
"%s: Falscher Typ für Eingabeargument #%d: Ein Makro oder eine Bibliothek "
"erwartet.\n"

#, c-format
msgid "%s: No more memory.\n"
msgstr "%s: Kein weiterer Speicher verfügbar.\n"

#, c-format
msgid "%s: Wrong number of output arguments: %d expected.\n"
msgstr "%s: Falsche Anzahl an Ausgabeargumenten: %d erwartet.\n"
