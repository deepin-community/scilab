msgid "%s: Wrong number of output argument(s): %d, %d or %d expected.\n"
msgstr "%s：输出参数数目错误：应该为%d个，%d个或%d个。\n"

#, c-format
msgid ""
"%s: Wrong value of hmin and hmax: hmin = %d is greater than hmax = %d.\n"
msgstr "%s：hmin和hmax的值错误：hmin =%d大于hmax =%d。\n"

