#, c-format
msgid "%s: Wrong number of input argument(s): %d expected.\n"
msgstr "%s: помилкова кількість вхідних параметрів: мало бути %d.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d : A matrix expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: мало бути використано матрицю.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d : Same size as argument %d expected.\n"
msgstr ""
"%s: помилкова розмірність вхідного параметра №%d: розмірність має збігатися "
"з розмірністю параметра №%d.\n"

#, c-format
msgid ""
"%s: Wrong type for input argument #%d : A tlist of type bsplin3val "
"expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: мало бути значення tlist типу "
"bsplin3val.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A %s tlist expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: слід використовувати типізований "
"список (tlist) %s.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d : A vector of size 3 expected.\n"
msgstr ""
"%s: помилкова розмірність вхідного параметра №%d: мало бути використано "
"вектор розмірності 3.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d : A real positive integer vector "
"expected.\n"
msgstr ""
"%s: помилкові значення вхідного параметра №%d: мало бути використано вектор "
"із додатних цілих чисел.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d : A matrix of size n x 3 expected.(n "
">= 10)\n"
msgstr ""
"%s: помилкова розмірність вхідного параметра №%d: мало бути вказано матрицю "
"розміру n x 3 (n >= 10).\n"

#, c-format
msgid "%s: Duplicate nodes or all nodes colinears (ier = %d).\n"
msgstr ""
"%s: дублювання вузлів або всі вузли лежать на одній прямій (ier = %d).\n"

#, c-format
msgid "%s: Wrong number of output argument(s): %d, %d or %d expected.\n"
msgstr ""
"%s: помилкова кількість вихідних параметрів: мало бути %d, %d або %d.\n"

#, c-format
msgid "%s: Wrong size for input arguments #%d and #%d: Same size expected.\n"
msgstr ""

#, c-format
msgid "%s: Wrong type for input argument #%d : A tlist of type %s expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: слід використовувати tlist типу "
"%s.\n"

#, c-format
msgid "%s: Wrong number of input argument(s): %d to %d expected.\n"
msgstr ""
"%s: помилкова кількість вхідних параметрів: слід використовувати від %d до "
"%d параметрів.\n"

#, c-format
msgid "%s: Wrong number of output argument(s): %d to %d expected.\n"
msgstr "%s: помилкова кількість вихідних параметрів: мало бути від %d до %d.\n"

#, c-format
msgid ""
"%s: Wrong size for input arguments #%d: A vector of size at least 2 "
"expected.\n"
msgstr ""
"%s: помилкова розмірність вхідних параметрів №%d: мало бути використано "
"вектор розмірності принаймні 2.\n"

#, c-format
msgid "%s: Wrong size for input arguments #%d to #%d: Same sizes expected.\n"
msgstr ""
"%s: помилкова розмірність вхідних параметрів від №%d до №%d: розмірність "
"мала збігатися.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d : string expected.\n"
msgstr ""
"%s: помилковий тип вхідного параметра №%d: мало бути використано рядковий "
"тип.\n"

#, c-format
msgid ""
"%s: Wrong values for input argument #%d : '%s' is an unknown '%s' type.\n"
msgstr ""
"%s: помилкові значення вхідного параметра №%d: «%s» належить до невідомого "
"типу «%s».\n"

#, c-format
msgid ""
"%s: Wrong size for input arguments #%d: A row vector of size at least 2 "
"expected.\n"
msgstr ""
"%s: помилкова розмірність вхідних параметрів №%d: мало бути використано "
"вектор-рядок розмірності принаймні 2.\n"

#, c-format
msgid ""
"%s: Wrong size for input arguments #%d: A column vector of size %d "
"expected.\n"
msgstr ""
"%s: помилкова розмірність вхідного параметра №%d: мало бути використано "
"вектор-стовпчик розмірності %d.\n"

#, c-format
msgid "%s: Wrong number of input argument(s): At least %d expected.\n"
msgstr ""
"%s: помилкова кількість вхідних параметрів: слід використовувати не менше за "
"%d.\n"

#, c-format
msgid "%s: Wrong size for input arguments #%d and #%d: Same sizes expected.\n"
msgstr ""
"%s: помилкова розмірність вхідних параметрів №%d і №%d: розмірність мала "
"збігатися.\n"

#, c-format
msgid ""
"%s: Wrong value for input argument #%d: Not (strictly) increasing or +-inf "
"detected.\n"
msgstr ""
"%s: помилкове значення вхідного параметра №%d: виявлено не(строге) зростання "
"або ±нескінченність.\n"

#, c-format
msgid ""
"%s: Size incompatibility between grid points and values in dimension %d.\n"
msgstr ""
"%s: несумісність розмірностей між точками сітки і значеннями у розмірності "
"%d.\n"

#, c-format
msgid ""
"%s: Size incompatibility between grid points and values in dimension %d or "
"%d.\n"
msgstr ""
"%s: несумісність розмірностей між точками сітки і значеннями у розмірності "
"%d або %d.\n"

#, c-format
msgid "%s: %s must be a real %d-dim hypermatrix.\n"
msgstr "%s: %s має бути %d-вимірною гіперматрицею з дійсних значень.\n"

#, c-format
msgid ""
"%s: Size incompatibility between grid points and grid values in dimension "
"%d.\n"
msgstr ""
"%s: несумісність розмірностей між точками сітки і значеннями сітки у "
"розмірності %d.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d : A vector of size 2 expected.\n"
msgstr ""
"%s: помилкова розмірність вхідного параметра №%d: мало бути використано "
"вектор розмірності 2.\n"

#, c-format
msgid "%s: Not enough points for the fit.\n"
msgstr "%s: недостатньо точок для інтерполяції.\n"

#, c-format
msgid "%ls: Warning: Rank deficiency of the least square matrix.\n"
msgstr ""
"%ls: попередження: недостатнє значення рангу для матриці методу найменших "
"квадратів.\n"

#, c-format
msgid "%s: Wrong value for input argument #%d: incorrect element #%d\n"
msgstr ""
"%s: помилкове значення вхідного параметра №%d: некоректний елемент №%d\n"

#, c-format
msgid "%s: last connected component of boundary is not closed\n"
msgstr "%s: останній з'єднаний компонент межі не замкнено\n"

#, c-format
msgid "%s: some points are identical.\n"
msgstr "%s: деякі з точок збігаються між собою.\n"

#, c-format
msgid "%s: wrong boundary array.\n"
msgstr "%s: помилковий масив межі.\n"

#, c-format
msgid "%s: crossed boundary.\n"
msgstr "%s: самоперетин межі.\n"

#, c-format
msgid "%s: wrong orientation of the boundary.\n"
msgstr "%s: помилкова орієнтація межі.\n"

#, c-format
msgid "%s: size limitation.\n"
msgstr "%s: обмеження на розмір.\n"

#, c-format
msgid "%s: an interior point is on the boundary.\n"
msgstr "%s: внутрішня точка перебуває на межі.\n"

#, c-format
msgid "%s: an interior point is too close to the boundary.\n"
msgstr "%s: внутрішня точка є надто близькою до межі.\n"

#, c-format
msgid "%s: some points are identical or size limitation.\n"
msgstr ""
"%s: деякі точки збігаються між собою або перевищено обмеження на розмір.\n"

#, c-format
msgid ""
"%s: Wrong size for input argument #%d : At least a size of 2 expected.\n"
msgstr ""
"%s: помилкова розмірність вхідного параметра №%d: слід використовувати "
"розмірність вхідного параметра рівну принаймні 2.\n"

#, c-format
msgid ""
"%s: Wrong size for input arguments #%d and #%d: Vector of same size "
"expected.\n"
msgstr ""
"%s: помилкова розмірність вхідних параметрів №%d і №%d: розмірності векторів "
"мали збігатися.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d : A matrix of size 2 expected.\n"
msgstr ""
"%s: помилковий вхідний параметр №%d: мала бути матриця розмірності 2.\n"

#, c-format
msgid "%s: Wrong value for periodic spline %s: Must be equal to %s.\n"
