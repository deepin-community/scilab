msgid "Wrong type for argument #%d: string expected."
msgstr "第%d个参数类型错误：应为字符串。"

#, c-format
msgid "Cannot open the given file %s."
msgstr "无法打给文件：%s。"

#, c-format
msgid "Wrong number of arguments : more than %d expected."
msgstr "参数数目错误：应该超过%d个。"

