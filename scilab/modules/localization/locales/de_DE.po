#, c-format
msgid "%s: Can not read input argument #%d.\n"
msgstr "%s: Eingangsargument #%d kann nicht gelesen werden.\n"

#, c-format
msgid "%s: Wrong size for input argument #%d: String expected.\n"
msgstr ""
"%s: Falsche Größe für Eingangsargument #%d: Es wird eine Zeichenkette "
"erwartet.\n"

#, c-format
msgid "%s: Memory allocation error.\n"
msgstr "%s: Speicherreservierungsfehler.\n"

#, c-format
msgid "%s: Unable to add new domain %s.\n"
msgstr "%s: Konnte neue Domain %s nicht hinzufügen.\n"

#, c-format
msgid "%s: No more memory.\n"
msgstr "%s: Kein weiterer Speicher verfügbar.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: String expected.\n"
msgstr "%s: Falscher Typ für Eingangsargument #%d: Zeichenkette erwartet.\n"

#, c-format
msgid "%s: Wrong number of input arguments: %d expected.\n"
msgstr "%s: Falsche Anzahl an Eingabeargumenten: %d erwartet.\n"

#, c-format
msgid "%s: Wrong number of output arguments: %d expected.\n"
msgstr "%s: Falsche Anzahl an Ausgabeargumenten: %d erwartet.\n"

#, c-format
msgid "%ls: This feature is only supported on Windows.\n"
msgstr "%ls: Diese Funktion wird nur unter Windows unterstützt.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: string expected.\n"
msgstr "%s: Falscher Typ für das Eingabeargument #%d: String erwartet.\n"

#, c-format
msgid "Unsupported language '%ls'.\n"
msgstr "Nicht unterstützte Sprache '%ls'.\n"

msgid "The language for menus cannot be changed on a running console.\n"
msgstr ""
"Die Sprache für Menüs kann nicht bei laufender Konsole geändert werden.\n"

msgid "Restart Scilab to apply to menus.\n"
msgstr "Starten Sie Scilab, um dies auf Menüs anzuwenden.\n"

#, c-format
msgid "%s: Wrong type for input argument #%d: A String expected.\n"
msgstr "%s: Falscher Typ für Eingabeargument #%d: Ein String wird erwartet.\n"

#, c-format
msgid "%ls: Unsupported language '%ls'.\n"
msgstr "%ls: Nicht unterstützte Sprache '%ls'.\n"

#, c-format
msgid "Switching to default language : '%ls'.\n"
msgstr "Zur Standardsprache wechseln: '%ls'.\n"
