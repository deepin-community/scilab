msgstr "%s：函数值数组中的列数为%d，但应该是1。"

#, c-format
msgid "%s: The number of rows in x is %d, while expected %d."
msgstr "%s：x中的行数为%d，但应该是%d。"

#, c-format
msgid "%s: The number of columns in x is %d, while expected %d."
msgstr "%s：x中的列数为%d，但应该是%d。"

