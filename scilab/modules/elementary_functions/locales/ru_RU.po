#, c-format
msgid "%s: Can not read input argument #%d.\n"
msgstr "%s: Не могу прочитать входной аргумент №%d.\n"

#, c-format
msgid ""
"%s: More than one argument can be used only with double matrix in first "
"argument.\n"
msgstr ""
"%s: Больше одного аргумента можно  использовать только с матрицей значений "
"типа double в первом аргументе.\n"

#, c-format
msgid "%ls:  Wrong type for input argument #%d: Real Matrix expected.\n"
